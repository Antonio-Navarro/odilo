DROP TABLE IF EXISTS todos;
 
CREATE TABLE todos (
  id INT AUTO_INCREMENT  PRIMARY KEY,
  userId INT NOT NULL,
  title VARCHAR(300) NOT NULL,
  completed BOOLEAN DEFAULT NULL
);
 
INSERT INTO todos (id, userId, title, completed) VALUES
  (1, 1, 'Pantalla de login', true),
  (2, 2', 'Pantalla de registro', true),
  (3, 1, 'Pantalla dashboard inicial', false);